    "lib": "lib"
  },
  "scripts": {
    "dev": "NO_D1_WARNING=true wrangler dev",
    "deploy": "NO_D1_WARNING=true wrangler publish",
    "db:create": "NO_D1_WARNING=true wrangler d1 create northwind",
    "db:createv3": "NO_D1_WARNING=true wrangler d1 create northwind --experimental",
    "db:delete": "NO_D1_WARNING=true wrangler d1 delete northwind",
    "db:init": "NO_D1_WARNING=true wrangler d1 execute northwind --file=../db/schema.sql",
    "db:load": "NO_D1_WARNING=true wrangler d1 execute northwind --file=../db/data.sql",
    "db:load-big": "NO_D1_WARNING=true wrangler d1 execute northwind --file=../db/data-big.sql"
  },
